{
  "name": "covercraft-extension",
  "version": "1.0.0",
  "description": "AI-powered cover letter generator Chrome extension",
  "scripts": {
    "dev": "concurrently \"webpack --mode development --watch\" \"node scripts/reload-server.js\"",
    "dev:fast": "webpack --mode development --watch --devtool eval-source-map",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "clean": "rm -rf dist",
    "zip": "cd dist && zip -r ../extension.zip .",
    "reload": "node scripts/reload-extension.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "axios": "^1.6.0",
    "lucide-react": "^0.263.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.23.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/chrome": "^0.0.248",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "typescript": "^5.2.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "ts-loader": "^9.5.0",
    "concurrently": "^8.2.2",
    "chokidar": "^3.5.3",
    "ws": "^8.14.2",
    "nodemon": "^3.0.1",
    "eslint": "^8.0.0"
  }
}